---
layout: layout.njk
title: 구단 분석
---

{% block styles %}
  <link rel="stylesheet" href="/style/team-base.css" />
{% endblock %}

{% block content %}
  <h1 class="page-title">구단 분석</h1>

  <div class="team-grid">
    {% for row in teams %}
      <div class="team-row">
        {% for team in row %}
          <a href="/teams/{{ team.code }}" class="team-box {{ team.code }}" title="{{ team.name }} 구단 분석">
            {{ team.name }}
          </a>
        {% endfor %}
      </div>
    {% endfor %}
  </div>
{% endblock %}